body,ul{padding:0}#newsletter h3,header #branding,header li{float:left}#boxes,header{padding-top:20px}#boxes .box,#social li a,footer{text-align:center}body{font:15px/1.5 Arial,Helvetica,sans-serif;margin:0;background-color:#EFE7E8}.container,.container-fluid{margin:auto;overflow:hidden}.container{width:80%}.container-fluid{width:90%}header #branding h1,ul{margin:0}.button_1{height:38px;background:#B42030;border:0;padding-left:20px;padding-right:20px;color:#FFF}#about,#boxes,#services,#showcase{background-color:#EFE7E8}#follow,#newsletter,header{background:#35424A}header{color:#fff;padding-bottom:10px;min-height:70px;border-bottom:#B42030 5px solid}.logo{position:absolute;top:15px;left:15px;width:50px;height:50px;border-radius:50%}.customer,.poultry{border:1px solid #000}header a{color:#fff;text-decoration:none;text-transform:uppercase;font-size:16px}header li{display:inline;padding:0 20px}header nav{float:right;margin-top:10px}.aboutUs .highlight,.ourService .highlight,.sidebar .highlight,header .current a,header .highlight{color:#B42030;font-weight:700}header a:hover{color:#ccc;font-weight:700}#showcase{width:100%;max-height:400px;overflow:hidden}.homePic{height:400px}#newsletter{padding:10px;color:#FFF}#newsletter form{float:right;margin-top:2px}#newsletter input[type=email]{padding:4px;height:35px;width:250px}#boxes .box{float:left;width:30%;padding:10px}#boxes .box img{width:150px;height:100px}.aboutUs,.ourService{margin-top:60px;width:60%}.poultry{margin-bottom:5px}#follow h3,#social{margin-bottom:0;float:left}.ourService{float:left}.customer{width:250px;height:180px}aside.sidebar{float:right;width:30%;margin-top:10px}div #mainCol{float:left;width:65%;margin:auto;overflow:hidden}#follow{color:#FFF;padding:5px}#follow h3{margin-top:10px}#social{padding:0 20px;display:flex}#social li{list-style-type:none;font-size:26px;margin:5px}#social li a{position:relative;width:40px;height:40px;display:block;border-radius:50%;background:#B42030;color:#35424A}footer{padding:10px 10px 5px;color:#FFF;background-color:#B42030}@media(max-width:768px){#boxes .box,#newsletter form,#newsletter h1,article#main-col,aside#sidebar,header #branding,header nav,header nav li{float:none;text-align:center;width:100%}header{padding-bottom:20px}#showcase h1{margin-top:40px}#newsletter button,.quote button{display:block;width:100%}#newsletter form input[type=email],.quote input,.quote textarea{width:100%;margin-bottom:5px}}
